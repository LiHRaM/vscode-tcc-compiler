{
	"name": "tcc-compiler-improved",
	"displayName": "TCC Compiler Improved",
	"description": "Improved version of TuyMove's TCC Compiler extension.",
	"version": "0.0.1",
	"publisher": "lihram",
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onCommand:extension.sayHello"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "tcc.run",
				"title": "TCC: Run"
			},
			{
				"command": "tcc.compile",
				"title": "TCC : Compile"
			}
		],
		"keybindings": [
			{
				"command": "tcc.run",
				"key": "F10"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	}
}