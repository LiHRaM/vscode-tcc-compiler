{
	"name": "tcc-compiler",
	"displayName": "TCC compile/run",
	"description": "compile C script with Tiny C Complier",
	"version": "0.0.4",
	"publisher": "TuyMove",
	"icon": "images/tcc-tuymove.png",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onCommand:tcc.run",
		"onCommand:tcc.compile"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "tcc.run",
				"title": "TCC : Run"
			},
			{
				"command": "tcc.compile",
				"title": "TCC : Compile"
			}
		],
		"keybindings": [
			{
				"command": "tcc.run",
				"key": "F10"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "0d18e1be-3e86-46d9-a8f1-262e3c388497",
		"publisherId": "b862fd1c-e7ad-4039-8219-24404cdfdf7a",
		"publisherDisplayName": "TuyMove"
	}
}